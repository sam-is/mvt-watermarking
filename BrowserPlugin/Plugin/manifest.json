{
  "name": "MVR request catcher",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "webRequest",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ] 
}